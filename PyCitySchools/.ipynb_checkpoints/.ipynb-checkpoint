   ],
   "source": [
    "Avg_math_score_by_type = school_data_complete.groupby(['type'])[\"math_score\"].mean()\n",
    "Avg_reading_score_by_type = school_data_complete.groupby(['type'])[\"reading_score\"].mean()\n",
    "student_by_type =  school_data_complete[\"type\"].value_counts()\n",
    "group_keys = (school_data_complete.type)\n",
    "Num_student_pass_math_by_type = ((school_data_complete['math_score'] >= 70).groupby(group_keys).sum()/student_by_type)*100\n",
    "group_keys = (school_data_complete.type)\n",
    "Num_student_pass_reading_by_type = ((school_data_complete['reading_score'] >= 70).groupby(group_keys).sum()/student_by_type)*100\n",
    "passing_rate_by_type = (school_data_complete.groupby(['type'])[\"math_score\"].mean() + school_data_complete.groupby(['type'])[\"reading_score\"].mean())/2\n",
    "\n",
    "# Creating a new DataFrame using school name, School type, Total students\n",
    "school_summary_table1 = pd.DataFrame({\"Average Math Score\": Avg_math_score_by_type,\n",
    "                                    \"Average Reading Score\": Avg_reading_score_by_type,\n",
    "                                    \"% Passing Math\": Num_student_pass_math_by_type,\n",
    "                                    \"% Passing Reading\": Num_student_pass_reading_by_type,\n",
    "                                    \"Overall Passing Rate\": passing_rate_by_type})\n",
    "\n",
    "#Improve formatting before printing\n",
    "school_summary_table1[\"Average Math Score\"] = school_summary_table1[\"Average Math Score\"].map(\"{0:,.0f}\".format)\n",
    "school_summary_table1[\"Average Reading Score\"] = school_summary_table1[\"Average Reading Score\"].map(\"{0:,.0f}\".format)\n",
    "school_summary_table1[\"% Passing Math\"] = school_summary_table1[\"% Passing Math\"].map(\"{0:,.3f}%\".format)\n",
    "school_summary_table1[\"% Passing Reading\"] = school_summary_table1[\"% Passing Reading\"].map(\"{0:,.3f}%\".format)\n",
    "school_summary_table1[\"Overall Passing Rate\"] = school_summary_table1[\"Overall Passing Rate\"].map(\"{0:,.0f}\".format)\n",
    "school_summary_table1\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
